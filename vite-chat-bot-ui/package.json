    "@tanstack/react-query-devtools": "^5.29.2",
    "can-ndjson-stream": "^1.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "framer-motion": "^11.1.4",
    "i": "^0.3.7",
    "install": "^0.13.0",
    "lucide-react": "^0.371.0",
